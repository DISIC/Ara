{
  "name": "confiture-rest-api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run prebuild && nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "show-all-ts-errors": "yarn g:show-all-ts-errors",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "postinstall": "run generate-api-types",
    "generate-api-types": "prisma generate && rimraf dist && GENERATE_TYPES=1 nest start --entryFile generate-api-typings.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.218.0",
    "@aws-sdk/s3-request-presigner": "^3.218.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.16.2",
    "@sentry/cli": "^2.57.0",
    "@sentry/nestjs": "^10.23.0",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express-http-proxy": "^2.0.0",
    "got": "^11.8.5",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mjml": "^4.14.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.7.7",
    "prisma": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.3",
    "slugify": "^1.6.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^9.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.194",
    "@types/mjml": "^4.7.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.10.0",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.31.1",
    "openapi-typescript": "^6.7.6",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": "^22.14.0"
  }
}
